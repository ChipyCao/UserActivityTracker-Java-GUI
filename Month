public class Month {
	int nbOfDaysUsed;
	int nbOfUsers;
	int users[];
	public Month(int nbOfDaysUsed, int nbOfUsers, int[] users) {
		super();
		this.nbOfDaysUsed = nbOfDaysUsed;
		this.nbOfUsers = nbOfUsers;
		this.users = users;
	}
	public int getNbOfDaysUsed() {
		return nbOfDaysUsed;
	}
	public void setNbOfDaysUsed(int nbOfDaysUsed) {
		this.nbOfDaysUsed = nbOfDaysUsed;
	}
	public int getNbOfUsers() {
		return nbOfUsers;
	}
	public void setNbOfUsers(int nbOfUsers) {
		this.nbOfUsers = nbOfUsers;
	}
	public int[] getUsers() {
		return users;
	}
	public void setUsers(int[] users) {
		this.users = users;
	}
	
}
